{
  "name": "emailbuddy",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "emailbuddy",
      "workspaces": [
        "apps/*",
        "packages/*"
      ]
    },
    "apps/companion": {
      "name": "@emailbuddy/companion",
      "version": "0.1.0",
      "dependencies": {
        "@emailbuddy/shared": "0.1.0"
      }
    },
    "apps/extension": {
      "name": "@emailbuddy/extension",
      "version": "0.1.0"
    },
    "node_modules/@emailbuddy/companion": {
      "resolved": "apps/companion",
      "link": true
    },
    "node_modules/@emailbuddy/extension": {
      "resolved": "apps/extension",
      "link": true
    },
    "node_modules/@emailbuddy/shared": {
      "resolved": "packages/shared",
      "link": true
    },
    "packages/shared": {
      "name": "@emailbuddy/shared",
      "version": "0.1.0"
    }
  }
}
